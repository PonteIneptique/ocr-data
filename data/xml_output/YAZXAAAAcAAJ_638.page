            <TextLine id="textline_33">
        
            <Coords points="2425,33 2425,1621 2505,1621 2505,33"/>
            <TextEquiv>
                <Unicode>Drückt den Geſtürzten nicht zu ſehr; &#39;s iſt Tugend.</Unicode>
