            <TextLine id="textline_22">
        
            <Coords points="1804,126 1804,334 1860,334 1860,126"/>
            <TextEquiv>
                <Unicode>Surr.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_23">
        
            <Coords points="1800,1252 1800,1504 1872,1504 1872,1252"/>
