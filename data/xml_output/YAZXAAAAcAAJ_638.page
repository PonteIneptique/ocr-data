            <TextLine id="textline_30">
        
            <Coords points="2287,45 2287,1105 2360,1105 2360,45"/>
            <TextEquiv>
                <Unicode>Mir nicht den Mund beſudeln will.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_31">
        
            <Coords points="2358,121 2358,378 2417,378 2417,121"/>
            <TextEquiv>
