            <TextLine id="textline_34">
        
            <Coords points="2498,33 2498,1654 2575,1654 2575,33"/>
            <TextEquiv>
                <Unicode>Klar vorm Geſetz liegt ſeine Schuld. Es ſtraf ihn;</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_35">
        
            <Coords points="2568,35 2568,1508 2640,1508 2640,35"/>
            <TextEquiv>
                <Unicode>Nicht aber ihr. Mein Herz weint, den ſo klein</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_36">
        
            <Coords points="2637,49 2637,1010 2710,1010 2710,49"/>
            <TextEquiv>
                <Unicode>Zu ſehn, der einſt ſo groß war.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_37">
        
            <Coords points="2704,1069 2704,1610 2781,1610 2781,1069"/>
            <TextEquiv>
                <Unicode>Ich vergeb&#39; ihm.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_38">
        
            <Coords points="2708,128 2708,343 2764,343 2764,128"/>
            <TextEquiv>
                <Unicode>Surr.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_39">
        
            <Coords points="2775,93 2775,1763 2848,1763 2848,93"/>
            <TextEquiv>
                <Unicode>Suff. Lord Kardinal, des Fernren will der König</Unicode>
