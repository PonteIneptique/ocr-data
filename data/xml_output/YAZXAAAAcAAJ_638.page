            <TextLine id="textline_41">
        
            <Coords points="2913,43 2913,1158 2988,1158 2988,43"/>
            <TextEquiv>
                <Unicode>Legat in dieſem Königreich ihr übtet,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_42">
        
            <Coords points="2981,37 2981,1378 3057,1378 3057,37"/>
            <TextEquiv>
                <Unicode>Jn den Bereich der Gutsverwirkung fällt -</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_43">
        
            <Coords points="3052,44 3052,1147 3128,1147 3128,44"/>
            <TextEquiv>
                <Unicode>Daß gegen euch ein Manifeſt erfolgt,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_44">
        
            <Coords points="3119,40 3119,1338 3195,1338 3195,40"/>
            <TextEquiv>
                <Unicode>Das eurer Güter, Länderein und Schätze,</Unicode>
