            <TextLine id="textline_1">
        
            <Coords points="304,1482 304,2459 404,2459 404,1482"/>
            <TextEquiv>
                <Unicode>Winter-Mährchen.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_2">
        
            <Coords points="478,277 478,1637 550,1637 550,277"/>
            <TextEquiv>
                <Unicode>Beſtiegen. Wenn auch während all der Zeit</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_3">
        
            <Coords points="545,276 545,1686 622,1686 622,276"/>
            <TextEquiv>
                <Unicode>Dir unſer Dank geworden wär&#39;, mein Bruder,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_4">
        
            <Coords points="613,280 613,1564 689,1564 689,280"/>
            <TextEquiv>
                <Unicode>So würden wir für alle Folg&#39;zeit doch als</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_5">
        
            <Coords points="684,276 684,1771 758,1771 758,276"/>
            <TextEquiv>
                <Unicode>Dein Schuldner ſcheiden. Drum gleich einer Null</Unicode>
            </TextEquiv>
