    <TextRegion id="textblock_0">
        <Coords points="0,0 0,3748 6120,3748 6120,3748"/>
        
            <TextLine id="textline_0">
        
            <Coords points="304,345 304,498 376,498 376,345"/>
            <TextEquiv>
                <Unicode>248</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_1">
        
            <Coords points="293,1459 293,2617 398,2617 398,1459"/>
            <TextEquiv>
                <Unicode>Ende gut, Alles gut.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_2">
        
            <Coords points="463,387 463,1474 537,1474 537,387"/>
            <TextEquiv>
                <Unicode>Erſt. Edelm. Ganz vortrefflich.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_3">
        
            <Coords points="532,374 532,2046 607,2046 607,374"/>
            <TextEquiv>
                <Unicode>Sold. (leſend). „Diana, der Graf iſt ein Narr und</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_4">
        
            <Coords points="601,332 601,899 678,899 678,332"/>
            <TextEquiv>
                <Unicode>ſchwer von Gold.“</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_5">
        
            <Coords points="668,380 668,2049 747,2049 747,380"/>
            <TextEquiv>
                <Unicode>Par. Das iſt nicht des Herzogs Brief, Herr, das iſt</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_6">
        
            <Coords points="738,309 738,2050 818,2050 818,309"/>
            <TextEquiv>
                <Unicode>eine Zuſchrift an ein gewiſſes Mädchen in Florenz, eine</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_7">
        
            <Coords points="808,308 808,2051 887,2051 887,308"/>
            <TextEquiv>
                <Unicode>gewiſſe Diana, ſich vor den Lockungen eines Grafen Rouſ-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_8">
        
            <Coords points="880,306 880,2050 956,2050 956,306"/>
            <TextEquiv>
                <Unicode>ſillon, eines albernen, eiteln Burſchen, der bei alldem</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_9">
        
            <Coords points="949,310 949,2052 1027,2052 1027,310"/>
            <TextEquiv>
                <Unicode>höchſt geil iſt, zu hüten. Ich bitte euch, ſteckt mir das</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_10">
        
            <Coords points="1019,329 1019,1277 1094,1277 1094,329"/>
            <TextEquiv>
                <Unicode>Schreiben wieder in die Taſche.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_11">
        
            <Coords points="1088,391 1088,1549 1161,1549 1161,391"/>
            <TextEquiv>
                <Unicode>Sold. Nein, erſt will ich ihn leſen.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_12">
        
            <Coords points="1156,379 1156,2051 1237,2051 1237,379"/>
            <TextEquiv>
                <Unicode>Par. Meine Abſicht dabei war, ſag&#39; ich, höchſt red-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_13">
        
            <Coords points="1226,310 1226,2053 1309,2053 1309,310"/>
            <TextEquiv>
                <Unicode>lich in Abſicht auf das Mädchen; denn ich weiß, daß der</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_14">
        
            <Coords points="1296,309 1296,2050 1376,2050 1376,309"/>
            <TextEquiv>
                <Unicode>junge Graf ein gefährlicher und ausſchweifender Geſell,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_15">
        
            <Coords points="1368,311 1368,2053 1444,2053 1444,311"/>
            <TextEquiv>
                <Unicode>ein wahrer Haifiſch der Jungfraunſchaft iſt, der alle Fiſch-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_16">
        
            <Coords points="1438,330 1438,1251 1513,1251 1513,330"/>
            <TextEquiv>
                <Unicode>brut verſchlingt, die er ſindet.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_17">
        
            <Coords points="1507,383 1507,2049 1583,2049 1583,383"/>
            <TextEquiv>
                <Unicode>Bert. Verdammenswerther, doppelzüngiger Schurke!</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_18">
        
            <Coords points="1574,382 1574,2053 1653,2053 1653,382"/>
            <TextEquiv>
                <Unicode>Sold. (leſend). „Schwört er, ſo fordre Geld von ihm</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_19">
        
            <Coords points="1648,799 1648,1230 1721,1230 1721,799"/>
            <TextEquiv>
                <Unicode>und nimm es;</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_20">
        
            <Coords points="1712,319 1712,1732 1796,1732 1796,319"/>
            <TextEquiv>
                <Unicode>„Ankreiden läßt er, zahlt doch nie die Kreide;</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_21">
        
            <Coords points="1785,312 1785,2054 1867,2054 1867,312"/>
            <TextEquiv>
                <Unicode>„Sei klug beim Handel drum, ſonſt wird dir Schlimmes,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_22">
        
            <Coords points="1853,316 1853,1943 1935,1943 1935,316"/>
            <TextEquiv>
                <Unicode>„Gemachte Schuld nicht zahlen, macht ihm Freude.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_23">
        
            <Coords points="1923,317 1923,1880 2003,1880 2003,317"/>
            <TextEquiv>
                <Unicode>„Dann, Diana, ſprich, wie wir&#39;s gerathen haben:</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_24">
        
            <Coords points="1994,314 1994,1982 2073,1982 2073,314"/>
            <TextEquiv>
                <Unicode>„Die Männer küßt man wohl, doch nicht die Knaben!</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_25">
        
            <Coords points="2065,316 2065,1896 2145,1896 2145,316"/>
            <TextEquiv>
                <Unicode>„Glaub mir, ein Narr iſt dieſer Graf, ich ſchwör&#39;,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_26">
        
            <Coords points="2131,322 2131,1675 2212,1675 2212,322"/>
            <TextEquiv>
                <Unicode>„Er zahlt voraus, doch niemahls hinterher.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_27">
        
            <Coords points="2204,412 2204,2056 2282,2056 2282,412"/>
            <TextEquiv>
                <Unicode>„Dein, wie ich dirs ins Ohr gelobt - Paroles.“</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_28">
        
            <Coords points="2273,382 2273,2060 2354,2060 2354,382"/>
            <TextEquiv>
                <Unicode>Bert. Er ſoll durch die Armee gepeitſcht werden, mit</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_29">
        
            <Coords points="2344,331 2344,1201 2418,1201 2418,331"/>
            <TextEquiv>
                <Unicode>dieſen Reimen an der Stirn.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_30">
        
            <Coords points="2412,385 2412,2058 2495,2058 2495,385"/>
            <TextEquiv>
                <Unicode>Zweit. Edelm. Das iſt euer treuergebener Freund,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_31">
        
            <Coords points="2484,313 2484,2063 2564,2063 2564,313"/>
            <TextEquiv>
                <Unicode>Herr Graf, der gewandte Sprachkenner, der waffentüch-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_32">
        
            <Coords points="2555,346 2555,730 2630,730 2630,346"/>
            <TextEquiv>
                <Unicode>tige Krieger!</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_33">
        
            <Coords points="2620,386 2620,2060 2702,2060 2702,386"/>
            <TextEquiv>
                <Unicode>Bert. Alles konnte ich bisher ertragen, nur keine</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_34">
        
            <Coords points="2692,333 2692,1339 2767,1339 2767,333"/>
            <TextEquiv>
                <Unicode>Katze, jetzt iſt er mir eine Katze.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_35">
        
            <Coords points="2763,388 2763,2058 2844,2058 2844,388"/>
            <TextEquiv>
                <Unicode>Sold. Aus den Blicken des Generals nehme ich ab,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_36">
        
            <Coords points="2831,315 2831,1606 2910,1606 2910,315"/>
            <TextEquiv>
                <Unicode>daß wir euch wohl werden hängen müſſen.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_37">
        
            <Coords points="2902,387 2902,2059 2979,2059 2979,387"/>
            <TextEquiv>
                <Unicode>Par. Um mein Leben bitt&#39; ich, Herr, und das jeden</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_38">
        
            <Coords points="2970,315 2970,2063 3054,2063 3054,315"/>
            <TextEquiv>
                <Unicode>Falles. Nicht daß ich mich vor dem Tode fürchte; aber</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_39">
        
            <Coords points="3041,319 3041,2063 3118,2063 3118,319"/>
            <TextEquiv>
                <Unicode>meiner Sünden ſind viele, ich möchte den Reſt meines</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_40">
        
            <Coords points="3113,318 3113,2059 3189,2059 3189,318"/>
            <TextEquiv>
                <Unicode>Lebens dazu verwenden, ſie zu bereuen. Laßt mich leben,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_41">
        
            <Coords points="3182,316 3182,2062 3260,2062 3260,316"/>
            <TextEquiv>
                <Unicode>Herr - im Kerker, im Stock oder ſonſt wo - wenn ich</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_42">
        
            <Coords points="3252,353 3252,624 3315,624 3315,353"/>
            <TextEquiv>
                <Unicode>nur lebe.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_43">
        
            <Coords points="3321,388 3321,2063 3398,2063 3398,388"/>
            <TextEquiv>
                <Unicode>Sold. Wir wollen ſehen, was ſich thun läßt, wenn</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_44">
        
            <Coords points="3390,318 3390,2067 3468,2067 3468,318"/>
            <TextEquiv>
                <Unicode>ihr freimüthig bekennt. Darum fragt man nochmahls hin-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_45">
        
            <Coords points="3461,319 3461,2065 3538,2065 3538,319"/>
            <TextEquiv>
                <Unicode>ſichtlich des Hauptmanns Dumain. Jn Betreff ſeiner Tap-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_46">
        
            <Coords points="3531,321 3531,2065 3607,2065 3607,321"/>
            <TextEquiv>
                <Unicode>ferkeit und ſeines Anſehens bei dem Herzoge habt ihr geant-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_47">
        
            <Coords points="3602,321 3602,1977 3679,1977 3679,321"/>
            <TextEquiv>
                <Unicode>wortet - „Wie ſieht es mit ſeiner Redlichkeit aus?“</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_48">
        
            <Coords points="3670,389 3670,2066 3748,2066 3748,389"/>
            <TextEquiv>
                <Unicode>Par. Er wird euch ein Ei aus einem Kloſter ſteh-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_49">
        
            <Coords points="3741,320 3741,2066 3815,2066 3815,320"/>
            <TextEquiv>
                <Unicode>len, Herr; denn im Rauben und Mädchenentführen kommt</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_50">
        
            <Coords points="3810,321 3810,2065 3900,2065 3900,321"/>
            <TextEquiv>
                <Unicode>er dem Neſus gleich. Er macht kein Geſchäft daraus,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_51">
        
            <Coords points="3880,320 3880,2069 3954,2069 3954,320"/>
            <TextEquiv>
                <Unicode>ſeine Schwüre zu halten, allein im Brechen derſelben iſt</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_52">
        
            <Coords points="3949,323 3949,2070 4030,2070 4030,323"/>
            <TextEquiv>
                <Unicode>er ſtärker als Herkules. Er wird mit ſolcher Zungenge-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_53">
        
            <Coords points="4019,321 4019,2068 4097,2068 4097,321"/>
            <TextEquiv>
                <Unicode>läufigkeit lügen, Herr, daß ihr glauben möchtet, die</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_54">
        
            <Coords points="4088,322 4088,2072 4164,2072 4164,322"/>
            <TextEquiv>
                <Unicode>Wahrheit ſei eine Närrin; Trunkenheit iſt ſeine beſte Tu-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_55">
        
            <Coords points="4159,322 4159,2072 4237,2072 4237,322"/>
            <TextEquiv>
                <Unicode>gend; denn er iſt ſaubetrunken; und im Schlafe thut er</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_56">
        
            <Coords points="4229,322 4229,2069 4306,2069 4306,322"/>
            <TextEquiv>
                <Unicode>am wenigſten Jemandem etwas zu Leide, außer ſeinen</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_57">
        
            <Coords points="4298,321 4298,2069 4377,2069 4377,321"/>
            <TextEquiv>
                <Unicode>Betttüchern. Allein man kennt ſeine Zuſtände und legt</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_58">
        
            <Coords points="4365,322 4365,2073 4447,2073 4447,322"/>
            <TextEquiv>
                <Unicode>ihn daher auf Stroh. Jch habe wenig mehr von ſeiner</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_59">
        
            <Coords points="4438,323 4438,2071 4516,2071 4516,323"/>
            <TextEquiv>
                <Unicode>Rechtſchaffenheit zu ſagen, Herr; er beſitzt Alles, was</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_60">
        
            <Coords points="4507,324 4507,2071 4588,2071 4588,324"/>
            <TextEquiv>
                <Unicode>ein rechtſchaffener Mann nicht beſitzen ſollte; von dem</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_61">
        
            <Coords points="4575,324 4575,2072 4656,2072 4656,324"/>
            <TextEquiv>
                <Unicode>aber, was ein rechtſchaffener Mann beſitzen ſollte, beſitzt</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_62">
        
            <Coords points="4644,363 4644,640 4718,640 4718,363"/>
            <TextEquiv>
                <Unicode>er nichts.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_63">
        
            <Coords points="4711,399 4711,2074 4793,2074 4793,399"/>
            <TextEquiv>
                <Unicode>Erſt. Edelm. Jch fange an, ihn um dieſer Aeuße-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_64">
        
            <Coords points="4785,349 4785,1179 4857,1179 4857,349"/>
            <TextEquiv>
                <Unicode>rung willen liebzugewinnen.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_65">
        
            <Coords points="4852,404 4852,2076 4934,2076 4934,404"/>
            <TextEquiv>
                <Unicode>Bert. Um dieſer Beſchreibung deiner Rechtſchaffenheit</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_66">
        
            <Coords points="4924,327 4924,2077 5006,2077 5006,327"/>
            <TextEquiv>
                <Unicode>willen? Die Peſt über ihn von meinetwegen; mir gilt er</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_67">
        
            <Coords points="4994,348 4994,1326 5069,1326 5069,348"/>
            <TextEquiv>
                <Unicode>nur um ſo mehr für eine Katze!</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_68">
        
            <Coords points="5064,401 5064,2074 5144,2074 5144,401"/>
            <TextEquiv>
                <Unicode>Sold. Was ſagt ihr von ſeiner Erfahrung im Kriegs-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_69">
        
            <Coords points="5133,367 5133,587 5202,587 5202,367"/>
            <TextEquiv>
                <Unicode>dienſte?</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_70">
        
            <Coords points="5199,403 5199,2075 5285,2075 5285,403"/>
            <TextEquiv>
                <Unicode>Par. Fürwahr, Herr, er hat vor den engländiſchen</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_71">
        
            <Coords points="5272,329 5272,2078 5351,2078 5351,329"/>
            <TextEquiv>
                <Unicode>Tragödienſpielern die Trommel her getragen - belügen</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_72">
        
            <Coords points="5343,329 5343,2080 5419,2080 5419,329"/>
            <TextEquiv>
                <Unicode>will ich ihn nicht - und mehr weiß ich nicht von ſeinem</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_73">
        
            <Coords points="5412,330 5412,2080 5491,2080 5491,330"/>
            <TextEquiv>
                <Unicode>Soldatenthum, ausgenommen daß er in jenem Lande, Eng-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_74">
        
            <Coords points="5483,331 5483,2083 5561,2083 5561,331"/>
            <TextEquiv>
                <Unicode>land, die Ehre hatte, an einem Orte, Meilenend ge-</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_75">
        
            <Coords points="5551,331 5551,2081 5631,2081 5631,331"/>
            <TextEquiv>
                <Unicode>nannt, Offizier zu ſein, und im Bilden doppelter Reihen</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_76">
        
            <Coords points="5619,330 5619,2082 5699,2082 5699,330"/>
            <TextEquiv>
                <Unicode>Unterricht zu geben. Gern möchte ich dem Mann alle</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_77">
        
            <Coords points="265,3415 265,3740 422,3740 422,3415"/>
            <TextEquiv>
                <Unicode>Akt IV.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_78">
        
            <Coords points="443,2019 443,3774 552,3774 552,2019"/>
            <TextEquiv>
                <Unicode>ihm gebührende Ehre erweiſen, aber über dieſen Umſtand</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_79">
        
            <Coords points="529,2040 529,2907 603,2907 603,2040"/>
            <TextEquiv>
                <Unicode>bin ich nicht genau berichtet.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_80">
        
            <Coords points="593,2091 593,3777 725,3777 725,2091"/>
            <TextEquiv>
                <Unicode>Erst. Edelm. Er hat die Spitzbüberei ſo ausgelernt,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_81">
        
            <Coords points="669,2034 669,3254 743,3254 743,2034"/>
            <TextEquiv>
                <Unicode>daß die Seltenheit ihn wieder losſpricht.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_82">
        
            <Coords points="722,2092 722,3777 822,3777 822,2092"/>
            <TextEquiv>
                <Unicode>Bert. Die Peſt über ihn; er hat doch eine Katzenatur</Unicode>
