            <TextLine id="textline_1">
        
            <Coords points="285,1382 285,2768 385,2768 385,1382"/>
            <TextEquiv>
                <Unicode>Johannis - Nachts - Traum.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_2">
        
            <Coords points="454,343 454,1803 533,1803 533,343"/>
            <TextEquiv>
                <Unicode>Doch hier, wo euch des Vaters Bill&#39;gung fehlt,</Unicode>
