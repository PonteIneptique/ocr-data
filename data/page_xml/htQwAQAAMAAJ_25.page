            <TextLine id="textline_20">
        
            <Coords points="2078,280 2078,2260 2164,2260 2164,280"/>
            <TextEquiv>
                <Unicode>zurückgekehrt iſt. - Wahrhaftig, Vetter, Du ſiehſt meiner armen</Unicode>
