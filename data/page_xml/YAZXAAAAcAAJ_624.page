            <TextLine id="textline_34">
        
            <Coords points="2481,116 2481,312 2540,312 2540,116"/>
            <TextEquiv>
                <Unicode>Buck.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_35">
        
            <Coords points="2475,1190 2475,1318 2544,1318 2544,1190"/>
