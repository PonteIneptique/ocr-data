            <TextLine id="textline_30">
        
            <Coords points="2182,21 2182,1491 2256,1491 2256,21"/>
            <TextEquiv>
                <Unicode>Ich ſag&#39; es nochmahls: keine Seel in England</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_31">
        
            <Coords points="2251,27 2251,1317 2326,1317 2326,27"/>
            <TextEquiv>
                <Unicode>Kann kräftiger euch lenken, als ihr ſelbſt.</Unicode>
