            <TextLine id="textline_12">
        
            <Coords points="1034,16 1034,1556 1111,1556 1111,16"/>
            <TextEquiv>
                <Unicode>Kann langſam nur geſchehn. Es gleicht der Zorn</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_13">
        
            <Coords points="1104,25 1104,1233 1180,1233 1180,25"/>
            <TextEquiv>
                <Unicode>Dem wilden Roß, das ungebändigt ſich</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_14">
        
            <Coords points="1171,21 1171,1372 1251,1372 1251,21"/>
            <TextEquiv>
                <Unicode>Durch eigne Kraft ermüdet. - Keiner räth</Unicode>
