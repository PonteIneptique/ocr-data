            <TextLine id="textline_16">
        
            <Coords points="1317,32 1317,879 1389,879 1389,32"/>
            <TextEquiv>
                <Unicode>Was ihr dem Freunde ſeid.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_17">
        
            <Coords points="1397,878 1397,1522 1475,1522 1475,878"/>
            <TextEquiv>
                <Unicode>Ich will zum König;</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_18">
        
            <Coords points="1403,116 1403,303 1461,303 1461,116"/>
            <TextEquiv>
                <Unicode>Buck.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_19">
        
            <Coords points="1469,22 1469,1381 1541,1381 1541,22"/>
            <TextEquiv>
                <Unicode>Der Ehre Mund ſoll dieſen frechen Burſchen</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_20">
        
            <Coords points="1536,16 1536,1555 1614,1555 1614,16"/>
            <TextEquiv>
                <Unicode>Aus Ipswich ganz zu Boden ſchrein, ſoll künden:</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_21">
        
            <Coords points="1610,28 1610,1177 1684,1177 1684,28"/>
            <TextEquiv>
                <Unicode>Nicht Unterſchied der Leute giebt&#39;s -</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_22">
        
            <Coords points="1692,1209 1692,1737 1767,1737 1767,1209"/>
            <TextEquiv>
                <Unicode>Nehmt Rath an:</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_23">
        
            <Coords points="1700,116 1700,326 1771,326 1771,116"/>
            <TextEquiv>
                <Unicode>Norf.</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_24">
        
            <Coords points="1761,21 1761,1377 1840,1377 1840,21"/>
            <TextEquiv>
                <Unicode>Heizt für den Feind den Ofen nicht ſo glüh,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_25">
        
            <Coords points="1831,22 1831,1431 1907,1431 1907,22"/>
            <TextEquiv>
                <Unicode>Daß er euch ſelbſt verſengt. Durch Haſtigkeit</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_26">
        
            <Coords points="1901,16 1901,1735 1972,1735 1972,16"/>
            <TextEquiv>
                <Unicode>Rennt man vorbei am Ziel, nach dem man rennt, und</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_27">
        
            <Coords points="1968,23 1968,1457 2047,1457 2047,23"/>
            <TextEquiv>
                <Unicode>Verfehlts durch Ueberrennen. Wißt ihr nicht,</Unicode>
            </TextEquiv>
            </TextLine>
            <TextLine id="textline_28">
        
            <Coords points="2041,19 2041,1493 2115,1493 2115,19"/>
            <TextEquiv>
                <Unicode>Daß Gluth, die&#39;s Waſſer durch das Ueberſieden</Unicode>
